{:name "rtable-aggrid"
 ; build
 :lazy true
 :depends-on #{:dali-dataset-transformer}
 :cljs-namespace [rtable.viewer.aggrid
                  rtable.transform.aggrid]
 :cljs-ns-bindings {'rtable.viewer.aggrid {'aggrid rtable.viewer.aggrid/aggrid}
                    'rtable.transform.aggrid {'load-and-transform-aggrid rtable.transform.aggrid/load-and-transform-aggrid}}
 ; theme
 :theme {:available  {:aggrid {true ["ag-grid-community/styles/ag-grid.css"]
                               "blue" ["ag-grid-community/styles/ag-theme-blue.css"]
                               "fresh" ["ag-grid-community/styles/ag-theme-fresh.css"]
                               "bootstrap" ["ag-grid-community/styles/ag-theme-bootstrap.css"]
                               "balham" ["ag-grid-community/styles/ag-theme-balham.css"]
                               "alpine-dark" ["ag-grid-community/styles/ag-theme-alpine-dark.css"]
                               "dark" ["ag-grid-community/styles/ag-theme-dark.css"]
                               "alpine" ["ag-grid-community/styles/ag-theme-alpine.css"]
                               "balham-dark" ["ag-grid-community/styles/ag-theme-balham-dark.css"]
                               "material" ["ag-grid-community/styles/ag-theme-material.css"]}}
         :current {:aggrid "alpine"}}}

